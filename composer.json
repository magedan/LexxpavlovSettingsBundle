{
    "name" : "lexxpavlov/settingsbundle",
    "description" : "Symfony2/3/4 Settings bundle provides flexible settings (Boolean, Integer, Float, String, Text, Html), easily configurable with Sonata Admin",
    "type" : "symfony-bundle",
    "homepage": "https://github.com/lexxpavlov/LexxpavlovSettingsBundle",
    "license" : "MIT",
    "keywords" : ["settings", "settings bundle", "settings management"],
    "authors" : [{
        "name" : "Alexey Pavlov",
        "email" : "lexx.pavlov@gmail.com"
    }],
    "require" : {
        "php": ">=7.1",
        "fresh/doctrine-enum-bundle": ">=4.8",
        "symfony/cache": "^3.4|^4.4|^5.0",
        "symfony/options-resolver": "^3.4|^4.4|^5.1",
        "symfony/form": "^3.4|^4.4|^5.1",
        "twig/twig": "^2.9|^3"
    },
    "suggest": {
        "sonata-project/admin-bundle": "Allow configure settings with administration panel",
        "egeloen/ckeditor-bundle": "Allow use ckeditor for Html value"
    },
    "autoload" : {
        "psr-4" : { "Lexxpavlov\\SettingsBundle\\" : "" }
    },
    "extra" : {
        "branch-alias" : {
            "dev-master" : "1.x-dev"
        }
    }
}
